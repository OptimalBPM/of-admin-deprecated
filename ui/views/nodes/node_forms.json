            "startEmpty": false,
            "options": {
                "multiple": "true",
                "httpPost": {
                    "url": "/node/lookup",
                    "parameter": {
                        "conditions": {"parent_id": "ObjectId(000000010000010001e64c24)"},
                        "collection": "node"
                    }
                }
            }
        },
        {
            "key": "runAs",
            "title": "Run as",
            "type": "strapselect",
            "description": "The user to run as.",
            "placeholder": "No users selected",
            "startEmpty": false,
            "options": {
                "multiple": "false",
                "httpPost": {
                    "url": "/node/lookup",
                    "parameter": {
                        "conditions": {"parent_id": "ObjectId(000000010000010001e64c25)"},
                        "collection": "node"
                    }
                }
            }
        }
    ],
